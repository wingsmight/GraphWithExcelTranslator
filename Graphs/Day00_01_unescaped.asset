    0000007D:
      type: {class: MonologueNode, ns: , asm: Assembly-CSharp}
      data:
        guid: a678e35a-f288-47d4-9540-bc342c1da251
        position: {x: 2293, y: 1386}
        texts:
        - "Зари, дорогая!
          Входи же, не стой
          у порога!"
        speakerName: Miam
    0000007E:
      type: {class: CharacterNode, ns: , asm: Assembly-CSharp}
